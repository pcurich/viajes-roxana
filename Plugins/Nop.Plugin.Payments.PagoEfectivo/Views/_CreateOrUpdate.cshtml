@model NotificationPagoEfectivoModel

<input asp -for= "Id" type = "hidden" />
<section class="content">
    <div class="container-fluid">
        <div class="form-horizontal">
            <div class="cards-group">
                <div class="card card-default">
                    <div class="card-body">
                        <div class="form-group row">
                            @if (Model.OrderId > 0)
                            {
                                <div class="col-md-4">
                                    <nop-label asp-for="OrderId" />
                                </div>
                                <div class="col-md-8">
                                    <nop-editor asp-for="OrderId" />
                                    <span asp-validation-for="OrderId"></span>
                                </div>
                            }
                        </div>
                        <div class="form-group row">
                            <div class="col-md-4">
                                <nop-label asp-for="Description" />
                            </div>
                            <div class="col-md-8">
                                <nop-editor asp-for="Description" />
                                <span asp-validation-for="Description"></span>
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="col-md-4">
                                <nop-label asp-for="Currency" />
                            </div>
                            <div class="col-md-8">
                                <nop-editor asp-for="Currency" />
                                <span asp-validation-for="Currency"></span>
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="col-md-4">
                                <nop-label asp-for="Amount" />
                            </div>
                            <div class="col-md-8">
                                <nop-editor asp-for="Amount"/>
                                <span asp-validation-for="Amount"></span>
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="col-md-4">
                                <nop-label asp-for="PhoneNumber" />
                            </div>
                            <div class="col-md-8">
                                <nop-editor asp-for="PhoneNumber" />
                                <span asp-validation-for="PhoneNumber"></span>
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="col-md-4">
                                <nop-label asp-for="PhoneCode" />
                            </div>
                            <div class="col-md-8">
                                <nop-editor asp-for="PhoneCode" />
                                <span asp-validation-for="PhoneCode"></span>
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="col-md-4">
                                <nop-label asp-for="DocumentType" />
                            </div>
                            <div class="col-md-8">
                                <nop-editor asp-for="DocumentType"/>
                                <span asp-validation-for="DocumentType"></span>
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="col-md-4">
                                <nop-label asp-for="DocumentTypeValue" />
                            </div>
                            <div class="col-md-8">
                                <nop-editor asp-for="DocumentTypeValue"/>
                                <span asp-validation-for="DocumentTypeValue"></span>
                            </div>
                        </div>
                        @if (Model.Id > 0)
                        {
                            <div class="form-group row">
                                <div class="col-md-4">
                                    <nop-label asp-for="OperationNumber" />
                                </div>
                                <div class="col-md-8">
                                    <nop-editor asp-for="OperationNumber" disabled="disabled"/>
                                    <span asp-validation-for="OperationNumber"></span>
                                </div>
                            </div>
                            <div class="form-group row">
                                <div class="col-md-4">
                                    <nop-label asp-for="TransactionCode" />
                                </div>
                                <div class="col-md-8">
                                    <nop-editor asp-for="TransactionCode" disabled="disabled"/>
                                    <span asp-validation-for="TransactionCode"></span>
                                </div>
                            </div>
                            <div class="form-group row">
                                <div class="col-md-4">
                                    <nop-label asp-for="Cip" />
                                </div>
                                <div class="col-md-8">
                                    <nop-editor asp-for="Cip" disabled="disabled"/>
                                    <span asp-validation-for="Cip"></span>
                                </div>
                            </div>
                            <div class="form-group row">
                                <div class="col-md-4">
                                    <nop-label asp-for="CipUrl" />
                                </div>
                                <div class="col-md-8">
                                    <nop-editor asp-for="CipUrl" disabled="disabled"/>
                                    <span asp-validation-for="CipUrl"></span>
                                </div>
                            </div>

                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>